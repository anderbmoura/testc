{
  "name": "@superapp-caixa/dsc-library",
  "version": "1.0.0",
  "description": "DSC Design System Components for React Native",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/module/index.d.ts",
  "react-native": "./src/index.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "react-native": "./src/index.ts",
      "import": "./lib/module/index.js",
      "require": "./lib/commonjs/index.js",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "bob build",
    "watch": "npm-watch",
    "clean": "rm -rf lib",
    "prepare-publish": "npm run build && node scripts/prepare-publish.js",
    "restore-dev": "node scripts/restore-dev.js",
    "publish-package": "npm run prepare-publish && npm publish && npm run restore-dev"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx,js,jsx"
    }
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-svg": "*",
    "react-native-gesture-handler": "*",
    "react-native-reanimated": "*",
    "react-native-worklets": "*",
    "tamagui": "*",
    "@tamagui/babel-plugin": "*",
    "@tamagui/build": "*",
    "@tamagui/config": "*",
    "@tamagui/core": "*",
    "iconoir-react-native": "*",
    "expo-blur": "*",
    "expo-linear-gradient": "*"
  },
  "devDependencies": {
    "react": "19.0.0",
    "react-native": "0.79.5",
    "@types/react": "~19.0.10",
    "husky": "^9.1.7",
    "npm-watch": "^0.13.0",
    "react-native-builder-bob": "^0.40.13",
    "typescript": "~5.8.3",
    "react-native-svg": "15.12.1",
    "react-native-gesture-handler": "2.28.0",
    "react-native-reanimated": "4.1.2",
    "react-native-worklets": "0.6.0",
    "tamagui": "1.132.21",
    "@tamagui/babel-plugin": "1.132.21",
    "@tamagui/build": "1.132.21",
    "@tamagui/config": "1.132.21",
    "@tamagui/core": "1.132.21",
    "iconoir-react-native": "7.11.0",
    "expo-blur": "15.0.7",
    "expo-linear-gradient": "14.0.2"
  },
  "author": "Leonardo de Albuquerque Gouveia",
  "license": "ISC",
  "homepage": "/rn-superapp-dsc-library#readme",
  "repository": {
    "type": "git",
    "url": "git+/rn-superapp-dsc-library.git"
  },
  "keywords": [
    "react-native",
    "DSC",
    "design-system",
    "components",
    "ui",
    "tamagui",
    "typescript",
    "ios",
    "android"
  ],
  "files": [
    "lib",
    "src",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "sideEffects": [
    "*.css"
  ],
  "publishConfig": {
    "registry": "https://pkgs.dev.azure.com/CAIXAAZURE/ComunidadeCanaisMobile/_packaging/caixa-react-native-feed/npm/registry/",
    "always-auth": true
  }
}
